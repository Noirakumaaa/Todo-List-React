{
  "name": "todo-listreact",
  "version": "1.0.0",
  "description": "Root directory for todo-listreact project",
  "scripts": {
    "setup" : "npm install",
    "install": "npm install --prefix server && npm install --prefix client",
    "build": "npm run build --prefix server && npm run build --prefix client",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "npm start --prefix server",
    "start:client": "npm run dev --prefix client"
  },
  "author": "Nathaniel Hernandez",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
